Entry{
  term = 'FQDN',
  expanded = 'Full Qualified Domain Name',
  meaning = 'vollständige Bezeichnung einer Adresse bestehend aus allen relevanten Namensbestandteilen (im Gegensatz zur URL ohne Pfadangabe)',
  scope = 'Internet, Domains'
}

Entry{
  term = 'Root-DN',
  expanded = 'Root Domain Name',
  meaning = 'der registrierte Domain Name bestehend aus dem Bezeichner (Marke, Name,...) und der Top Level Domain (TLD) bzw. dem Public Suffix',
  scope = 'Internet, Domains'
}
 
Entry{
  term = 'URL',
  expanded = 'Uniform Resource Locator',
  meaning = 'komplette Adresse einer Information im Internet, bestehend aus der Protokollangabe, dem FQDN (=Server-Zuordnung) und einer Pfadangabe einschließlich der zu benennenden Datei (z.B. http://lanxess.com/test/xyz.html)',
  scope = 'Internet'
}

Entry{
  term = 'TLD',
  expanded = 'Top Level Domain',
  meaning = 'oberste Ebene der Namenshierarchie im Domain Namen (das äußerste rechte Namenspartikel abgetrennt durch einen Punkt). Es wird in verschiedene Typen unterschieden (gTLD, ccTLD oder ngTLD), die aber alle von der ICANN genehmigt und freigegeben sein müssen.',
  scope = 'Internet'
}

Entry{
  term = 'gTLD',
  expanded = 'Generic Top Level Domain',
  meaning = 'allgemeine Top Level Domain (TLD) wie "COM", "NET", "ORG" etc.',
  scope = 'Internet'
}

Entry{
  term = 'ccTLD',
  expanded = 'Country Code Top Level Domain',
  meaning = 'Länderkennungen wie z.B. "US", "DE", "CA", etc.',
  scope = 'Internet'
}

Entry{
  term = 'ngTLD',
  expanded = 'New Generic Top Level Domain',
  meaning = 'spezielle TLD, die üblicherweise von Firmen für besondere Zwecke erwünscht und von der ICANN freigegeben worden sind. Neben Bezeichnungen für spezielle Interessengruppen finden sich hierunter in der Hauptsache Marken- oder Firmennamen. Beispiele: "AERO", "CLUB", "LANXESS" etc.',
  scope = 'Internet'}

Entry{
  term = 'Public Suffix',
  meaning = 'da nicht alle Regierungen zulassen, dass normale Domainregistrierungen direkt unter der Landeskennung (ccTLD) erfolgen, wurden für diese Länder von den dortigen Registrierungsbehörden zusätzliche Präfixe für Nutzergruppen eingerichtet. Registrierbar sind dann Namen bestehend aus dem Bezeichner + Präfix + ccTLD. Ähnliches gilt auch für einige ngTLD. Die Kombination aus Präfix und ccTLD oder ngTLD wird dann als "public suffix" bezeichnet.',
  scope = 'Internet'
}

Entry{
  term = 'ICANN',
  expanded = 'Internet Corporation for Assigned Names and Numbers',
  meaning = 'die oberste Kontroll-Organisation für alle Fragen bzgl. der Organisation des Internet, speziell für die Vergabe der Domain Namen.',
  scope = 'Internet'}

Entry{
  term = 'ARIN',
  expanded = 'American Registry for Internet Numbers',
  meaning = 'Vergabestelle für die numerischen Adressen für Maschinen (**IP-Adressen**). Vergeben werden Adressbereiche an Firmen oder Länderorganisationen (z.B. RIPE für Europa) zur Weitergabe einzelner Adressen an Betreiber von Maschinen oder Diensten.',
  scope = 'Internet'
}

Entry{
  term = 'IP-Adresse',
  meaning = 'eindeutige numerische Adresse einer Maschine im Internet, im alten Format (IPv4) bestehend aus vier Ziffernblöcken (z.B. 255.255.255.255) und im neuen Format (IPv6) bestehend aus acht Hexadezimalblöcken (z.B. 2001:0db8:85a3:08d3:1319:8a2e:0370:7347)',
  scope = 'Internet'
}

Entry{
  term = 'DNS',
  expanded = 'Domain Name System',
  meaning = 'Dienst im Internet zur Umwandlung von Namen (FQDN) in IP-Adressen (Serveradressen). Damit einer der wichtigsten Dienste im Internet. Neben der Adressauflösung liefert der DNS weitere Informationen zu den gespeicherten Namen. ',
  scope = 'Internet'
}

Entry{
  term = 'Zone',
  meaning = 'im DNS werden alle Informationen (Einträge zur Domain und den Subdomains) zu einem **Root-DN** gemeinsam abgelegt, der so entstehende Datenblock wird als "Zone" bezeichnet.',
  scope = 'Internet'
}

Entry{
  term = 'WHOIS',
  meaning = 'Datenbank der Registry, in der zu jedem registrierten DN Inhaber- und Ansprechpartner-Daten gespeichert sind, sowie Angaben zur Transfer-Fähigkeit und zu den authoritativen Nameservern.',
  scope = 'Internet'
}

Entry{
  term = 'Transfer Lock',
  meaning = 'Kennzeichnung im WHOIS der Registry zu einem DN, die einen versehentlichen Transfer ausschließt.',
  scope = 'Internet'
}

Entry{
  term = 'Lapse',
  meaning = 'Kennzeichnung beim Provider und ggf. bei der Registry zu einem DN, die eine automatische Renewal ausschließt und ein Auslaufen der Registrierung ankündigt.',
  scope = 'Internet'
}

Entry{
  term = 'Authorization Code',
  meaning = 'von der Registry erzeugter und nur dem Eigentümer bekanntgegebener Sicherheitsschlüssel, der beim Transfer an den neuen Eigentümer weitergegeben werden muss, um den Transfer zum neuen Provider/Eigentümer zu ermöglichen.',
  scope = 'Internet'
}

Entry{
  term = 'Authoritative Nameserver',
  meaning = 'zu jedem Root-DN gehört ein DNS-Datenblock, der zumindestens festlegt, wo DNS-Abrufe nach gültigen und vertrauenswürdigen Start-Informationen suchen können. Die betreffenden Nameserver werden als authoritative Nameserver bezeichnet und üblicherweise vom Provider vergeben. Es werden mindestens zwei dieser Server im WHOIS als Startpunkt für DNS-Abfragen hinterlegt.',
  scope = 'Internet'
}

Entry{
  term = 'GDPR',
  expanded = 'General Data Protection Regulation',
  alternates = 'auch _EU-DSGVO_ oder _Datenschutz-Grundverordnung_',
  meaning = 'Die Europäische Datenschutzregelung vom 25. Mai 2018 hat starken Einfluss auch auf die Speicherung und Weitergabe von Eigentümer- und Kontaktdaten zu Internet Domain Names. Sie verhindert es, dass bei Fremdregistrierung von Domains mit markenrechtlich geschütztem Bezeichner oder mit Verweis auf schädigende Web-Inhalte auf einfache Weise der Registrant der Domain ermittelt und zur Rechenschaft gezogen werden kann. Zugang können nur noch bestimmte Personenkreise unter strengem Nachweis der unausweichlichen Notwendigkeit erhalten.',
  scope = 'Internet'
}

Entry{
  term = 'NIC',
  expanded = 'Network Information Center',
  meaning = 'Bezeichnung für die Vertretung und meist auch Online-Präsenz einer Registry',
  scope = 'Internet'
}

Entry{
  term = 'Registry',
  meaning = 'Jede Registry ist bei der ICANN akkreditiert und betreut eine oder mehrere TLD´s. Provider, die entsprechende Domains vermitteln möchten, müssen wiederum bei der jeweiligen Registry akkrediert sein.',
  scope = 'Internet'
}

Entry{
  term = 'Registrar',
  meaning = 'Provider, über den Domains registriert werden können und der normalerweise zu diesem Zweck bei diversen Registries bzw. NIC´s akkreditiert ist.',
  scope = 'Internet'
}

Entry{
  term = 'Registrant',
  meaning = 'Eigentümer eines Domain Name',
  scope = 'Internet'
}

Entry{
  term = 'Transfer',
  meaning = 'Übertragung der Eigentumsrechte an einem Domain Namen',
  scope = 'Internet'
}

Entry{
  term = 'UDRP',
  expanded = 'Unified Dispute Resolution Procedure',
  meaning = 'übliches Verfahren, um Eigentumsrechte an einem Domain Namen anzufechten.',
  scope = 'Internet'
}

Entry{
  term = 'Cease & Desist',
  meaning = 'Verfahren, um beispielsweise bei Markenrechtsverletzungen oder Hass-Webseiten auf kurzem Wege die Nutzung eines Domain Name zu untersagen.',
  scope = 'Internet'
}

Entry{
  term = 'Resource Record',
  meaning = 'Eintrag im DNS mit einem spezifischen Typ, der z.B. von einem Browser abgefragt werden kann',
  scope = 'Internet'
}

Entry{
  term = 'TTL',
  expanded = 'Time To Live',
  meaning = '"Lebensdauer" eines Eintrags im DNS, nach Ablauf wird der aktuelle Inhalt neu ausgerollt (DNS Propagation)',
  scope = 'Internet'
}

Entry{
  term = 'DNS Propagation',
  meaning = 'Das DNS-System besteht aus verteilten Servern, damit Anfragen zügig beantwortet werden können und der Ausfall eines Servers keine Probleme bereitet. Änderungen werden auf den authoritativen Name Servern vorgenommen und werden dann an die anderen Server im Internet weiter verteilt.',
  scope = 'Internet'
}

Entry{
  term = 'IPv4',
  meaning = 'Die Adressierung von Resourcen (Servern etc.) im Internet erfolgt technisch über eine numerische Adresse (ähnlich einer Telefonnummer). Diese Adresse war ursprünglich 32 Bit groß und wird noch heute weitgehend verwendet. Da der Nummernkreis zu klein ist, wurde bereits eine neue Norm eingeführt (IPv6)',
  scope = 'Internet'
}

Entry{
  term = 'IPv6',
  meaning = 'Als Nachfolger von IPv4 von der IETF standardisiert, hat diese Adresse für Resourcen im Internet eine Größe von 128 Bit.',
  scope = 'Internet'
}

Entry{
  term = 'IETF',
  expanded = 'Internet Engineering Task Force',
  meaning = 'internationale Community zur Standardisierung der für das Internet verwendeten Technologien, begonnen mit den Netzwerk-Protokollen bis hin zu den verschiedenen Diensten im Internet (e-Mail, http/https, ...). Standards werden in RFC´s dokumentiert und verwaltet. Der Internet Standards Process mit den Aufgaben der IETF ist beschrieben in [RFC 2026](https://datatracker.ietf.org/doc/rfc2026 "The Internet Standards Process -- Revision 3")[@RFC2026]',
  scope = 'Internet'
}

Entry{
  term = 'IANA',
  expanded = 'Internet Assigned Numbers Authority',
  meaning = 'zentraler Koordinator für die Zuweisung der einzelnen Parameter für Internet-Protokolle, beispielsweise steuert die IANA die Vergabe der IP-Adressen blockweise an die Provider',
  scope = 'Internet'
}

Entry{
  term = 'RFC',
  expanded = 'Request for Comments',
  meaning = 'Dokumentation für Internet-Standards aller Art, sowohl für technische Standards als auch für Verfahren und Organisation.',
  scope = 'Internet'
}

